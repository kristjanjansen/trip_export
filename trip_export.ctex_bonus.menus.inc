<?php

/**
 * @file
 * Bulk export of menus generated by Drush Ctools Export Bonus.
 * Use drush cbrme modulename to rebuild menus.
 */

/**
 * Implements hook_drush_ctex_bonus_menus().
 */
function trip_export_drush_ctex_bonus_menus() {
  $menus = array();

  $menus['main-menu'] = (object) array(
    'menu_name' => 'main-menu',
    'title' => 'Main menu',
    'description' => 'The <em>Main</em> menu is used on many sites to show the major sections of the site, often in a top navigation bar.',
    'links' => array(),
  );

  return $menus;
}
